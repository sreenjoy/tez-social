{
  "version": 2,
  "name": "tez-social-frontend",
  "buildCommand": "npm run build",
  "outputDirectory": "out",
  "functions": {
    "api/*.js": {
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "routes": [
    {
      "src": "^/api/(.*)$",
      "dest": "/api/$1"
    },
    {
      "src": "^/static/(.*)$",
      "dest": "/static/$1"
    },
    {
      "src": "^/[^.]+$",
      "dest": "/index.html"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
} 